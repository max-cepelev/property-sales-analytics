    "@types/node": "^20.1.3",
    "@types/pdf-parse": "^1.1.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prisma": "^4.14.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/env": "^4.2.0",
