{
  "name": "server",
  "version": "1.0.0",
  "description": "fastify-server-example",
  "main": "app.ts",
  "types": "module",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "export NODE_ENV=development || set NODE_ENV=development && yarn nodemon",
    "start": "export NODE_ENV=production || set NODE_ENV=production && node dist/app.js",
