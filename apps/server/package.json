{
  "name": "server",
  "version": "0.0.1",
  "description": "fastify-server-example",
  "main": "app.ts",
  "types": "module",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "export NODE_ENV=development || set NODE_ENV=development && yarn nodemon",
    "start": "export NODE_ENV=production || set NODE_ENV=production && node dist/app.js",
    "test": "test",
    "lint": "npx eslint src",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier -- --write",
    "format": "yarn prettier:fix && npm run lint:fix"
  },
  "author": "Maksim Tsepelev",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/env": "^4.2.0",
    "@fastify/jwt": "^6.7.1",
    "@fastify/redis": "^6.1.1",
    "@fastify/static": "^6.10.1",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.0",
    "@pothos/core": "^3.29.0",
    "@pothos/plugin-prisma": "^3.47.2",
    "@prisma/client": "4.12.0",
    "argon2": "^0.30.3",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "fastify-type-provider-zod": "^1.1.9",
    "got": "^12.6.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.9.0",
    "pdfjs-dist": "^3.5.141",
    "zod": "^3.21.4"
  }
