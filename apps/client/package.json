		"@mui/x-date-pickers": "^6.2.0",
		"axios": "^1.3.5",
		"dayjs": "^1.11.7",
		"graphql": "^16.6.0",
		"notistack": "^3.0.1",
		"pigeon-maps": "^0.21.3",
